{
  "manifest_version": 2,
  "name": "DDG Position",
  "version": "1.0",
 
  "description": "Places a marker with the position a result in Google appears on duckduckgo for the same search term.",
 
  "content_scripts": [
    {
      "matches": ["*://*.google.com/search?*"],
      "js": ["script.js"]
    }
  ],
 
  "permissions": [
    "*://duckduckgo.com/html/*",
    "*://html.duckduckgo.com/html/*",
    "*://localhost/",
    "webRequest"
  ]
}
